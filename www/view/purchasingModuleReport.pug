include ./global/head.pug
include ./parts/sidenav.pug
main.main
  include ./parts/main-header.pug
  include ./parts/main-subheader.pug
  
  .main_content(ng-cloak)
    .page_header
      h2 Report Modul Pembelian
    .page_content
      .row
        .col-md-3.col-lg-3.col-sm-4.col-xs-6(ng-repeat="submodule in submoduleList")
          a.tile.tile--selectable.tile--primary.tile--equal(href="{{submodule.link}}")
            .tile_content
              .tile_main
                h3 {{submodule.name}}
