include ./global/head.pug
include ./parts/sidenav.pug
main.main
	include ./parts/main-header.pug
	include ./parts/main-subheader.pug
	
	.main_content(ng-cloak)
		.page_header
			h2(role="page-title")= page.title
		.page_content
			.row
				.col-md-3.col-lg-3.col-sm-4.col-xs-6(ng-if="tile.show" ng-repeat="tile in tileList")
					a.tile.tile--default.tile--selectable.tile--equal(href="{{tile.link}}")
						.tile_content
							i.icon.icon--colossal(ng-class="tile.icon")
							h5 {{tile.name}}
